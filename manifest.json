{
    "manifest_version": 2,
    "name": "Notepad Extension",
    "version": "1.0",
    "description": "A basic notepad Chrome extension",
    "permissions": ["storage"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "manifest_version": 2
  }
  